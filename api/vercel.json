{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": { 
        "includeFiles": ["./package.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/",
      "methods": ["GET", "POST", "OPTIONS"]
    }
  ]
}